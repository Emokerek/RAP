///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////   


        if(abc>xyz){                //gdy abc jest wiÄ™ksze niÅ¼ xyz
            
            while(abc%xyz!=0){      //sprawdza czy reszta to 0
                
                int r=abc%xyz;      //oblicza resztÄ™ z dzielenia
                printf("%d\n",r);   
                abc=xyz;            //wykonuje zamianÄ™ abcâ†xyz
                printf("%d\n",abc);
                xyz=r;              //wykonuje zamianÄ™ xyzâ†r
                printf("%d\n\n\n",xyz);
            }
            
        printf("NajwiÄ™kszy wspÃ³lny dzielnik to: %d",xyz);
        }
        
        else                        //gdy xyz>abc albo sÄ… rÃ³wne sobie
        {
            while(xyz%abc!=0){      //sprawdza czy reszta to 0
                
                int r=xyz%abc;      //oblicza resztÄ™ z dzielenia
                printf("%d\n",r);
                xyz=abc;            //wykonuje zamianÄ™ xyzâ†abc
                printf("%d\n",xyz);         
                abc=r;              //wykonuje zamianÄ™ abcâ†r
                printf("%d\n\n\n",abc);
            }
            
        printf("NajwiÄ™kszy wspÃ³lny dzielnik to: %d",abc);    
        }
        
