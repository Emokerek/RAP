#include <stdio.h>

int main()
{
    int abc , xyz;
    printf("Podaj liczby po spacji:");
    scanf("%d %d",&abc, &xyz);
    int r, temp;
    
    
///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////    
/*
KROK 1
Podziel 𝑚 przez 𝑛 i niech 𝑟 oznacza resztę z tego dzielenia.  "m" to "abc", "n" to "xyz"
KROK 2
Jeśli reszta 𝑟=0, zakończ algorytm; odpowiedzią jest 𝑛.
KROK 3
Wykonaj zamianę 𝑚←𝑛, 𝑛←𝑟 i wróć do kroku1.
*/
///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////   


        if(xyz>abc){                //gdy xyz jest większe niż abc, wykonuje zamianę
            temp=xyz;
            xyz=abc;
            abc=temp;           
        }
        
        while(abc%xyz!=0){      //sprawdza czy reszta to 0
                
                int r=abc%xyz;      //oblicza resztę z dzielenia
                printf("%d\n",r);   
                abc=xyz;            //wykonuje zamianę abc←xyz
                printf("%d\n",abc);
                xyz=r;              //wykonuje zamianę xyz←r
                printf("%d\n\n\n",xyz);
            }
        
        
        
    return 0;
}
