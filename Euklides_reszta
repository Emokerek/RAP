///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////   


        if(xyz>abc){                //gdy xyz jest wiÄ™ksze niÅ¼ abc, wykonuje zamianÄ™
            temp=xyz;
            xyz=abc;
            abc=temp;           
        }
        
        while(abc%xyz!=0){      //sprawdza czy reszta to 0
                
                int r=abc%xyz;      //oblicza resztÄ™ z dzielenia 
                abc=xyz;            //wykonuje zamianÄ™ abcâ†xyz
                xyz=r;              //wykonuje zamianÄ™ xyzâ†r
         } 
         
         printf("NajwiÄ™kszy wspÃ³lny dzielnik to: %d", xyz);
            
    return 0;
}
