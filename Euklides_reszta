#include <stdio.h>

int main()
{
    int abc , xyz;
    printf("Podaj liczby po spacji:");
    scanf("%d %d",&abc, &xyz);
    printf("Twoja liczby to: %d %d\n",abc, xyz);
    int r;
    
    
///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////    
/*
KROK 1
